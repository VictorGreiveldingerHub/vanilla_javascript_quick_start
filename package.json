{
  "name": "vanilla_javascript_quick_start",
  "version": "1.0.0",
  "description": "Template Vanilla Javascript",
  "main": "app.js",
  "scripts": {
    "backend:build": "node app.js",
    "backend:development": "nodemon app.js",
    "frontend:build": "webpack --progress --config webpack.config.build.js",
    "frontend:development": "webpack serve --progress --config webpack.config.development.js",
    "start": "concurrently --kill-others \"npm run backend:development\" \"npm run frontend:development\"",
    "build": "npm run frontend:build",
    "start:prod": "npm run frontend:build && npm run backend:build",
    "serve:prod": "concurrently --kill-others \"npm run backend:build\" \"npm run frontend:build\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/VictorGreiveldingerHub/vanilla_javascript_quick_start.git"
  },
  "keywords": [
    "Javascript",
    "Vanilla",
    "WEBGL",
    "Shader"
  ],
  "author": "Victor Greiveldinger",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VictorGreiveldingerHub/vanilla_javascript_quick_start/issues"
  },
  "homepage": "https://github.com/VictorGreiveldingerHub/vanilla_javascript_quick_start#readme",
  "dependencies": {
    "babel-loader": "^10.0.0",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "nodemon": "^3.1.10",
    "path": "^0.12.7",
    "pug": "^3.0.3",
    "sass": "^1.93.2",
    "webpack": "^5.102.1"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "glslify-loader": "^2.0.0",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss-loader": "^8.2.0",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  }
}
